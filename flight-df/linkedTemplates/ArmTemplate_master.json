{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "flight-df"
		},
		"SalesCsvDataLakeStorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'SalesCsvDataLakeStorage'"
		},
		"SalesDataStorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'SalesDataStorage'"
		},
		"SalesStagingTableDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SalesStagingTableDB'"
		},
		"SalesCsvDataLakeStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dbfabric.dfs.core.windows.net/"
		},
		"SalesDataStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dbfabric.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "flight-df_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"SalesCsvDataLakeStorage_accountKey": {
						"value": "[parameters('SalesCsvDataLakeStorage_accountKey')]"
					},
					"SalesDataStorage_accountKey": {
						"value": "[parameters('SalesDataStorage_accountKey')]"
					},
					"SalesStagingTableDB_connectionString": {
						"value": "[parameters('SalesStagingTableDB_connectionString')]"
					},
					"SalesCsvDataLakeStorage_properties_typeProperties_url": {
						"value": "[parameters('SalesCsvDataLakeStorage_properties_typeProperties_url')]"
					},
					"SalesDataStorage_properties_typeProperties_url": {
						"value": "[parameters('SalesDataStorage_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}